                  <res>[1,1,1]</res>
                  <velocity>[0.0,0.0,0]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material> -->


           <contact>
             <type>rigid</type>
             <filename>rigid_tri_IS.txt</filename>
	     <master_material>0</master_material>
             <direction>[0,1,0]</direction>
	     <materials>[0,1]</materials>
           </contact> 

      	   <contact>
             <type>single_velocity</type>
	     <materials>[1,2]</materials>
           </contact>

       </MPM>
    </MaterialProperties>

    <PhysicalBC>
     <MPM>

	   
       <pressure>
	 <outward_normal>true</outward_normal>
         <geom_object>
            <box label="x-face">
              <min>[0,0.0,0.01]</min>
              <max>[0.001,0.01,0.02]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>1</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5   </time>
             <load> 10000 </load>
           </time_point>
           <time_point>
             <time> 100.0   </time>
             <load> 10000 </load>
           </time_point>
         </load_curve>
       </pressure>

       <pressure>
	<outward_normal>true</outward_normal>
         <geom_object>
            <box label="x+face">
              <min>[0.029,0.0,0.01]</min>
              <max>[0.03,0.01,0.02]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>2</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5</time>
             <load> -10000 </load>
           </time_point>
           <time_point>
             <time> 50.0   </time>
             <load> -10000  </load>
           </time_point>
         </load_curve>
       </pressure>

       <pressure>
         <geom_object>
            <box label="z-face">
              <min>[0.01,0.0,0.0]</min>
              <max>[0.02,0.01,0.001]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>3</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5 </time>
             <load> 10000 </load>
           </time_point>
           <time_point>
             <time> 50.0   </time>
             <load> 10000 </load>
           </time_point>
         </load_curve>
       </pressure>

       <pressure>
         <geom_object>
            <box label="z+face">
              <min>[0.01,0.0,0.029]</min>
              <max>[0.02,0.01,0.03]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>4</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5 </time>
             <load> -10000 </load>
           </time_point>
           <time_point>
             <time> 50.0   </time>
             <load> -10000 </load>
           </time_point>
         </load_curve>
       </pressure>

     </MPM>
   </PhysicalBC>

       
    <Grid>
       <BoundaryConditions>

          <Face side = "x-">
             <BCType id = "all" label = "Symmetric" var = "symmetry"> </BCType>
           </Face>                  
          <Face side = "x+">
             <BCType id = "all" label = "Symmetric" var = "symmetry"> </BCType>
           </Face>
           <Face side = "y-">
             <BCType id = "all" label = "Symmetric" var = "symmetry"> </BCType>
           </Face>                  
          <Face side = "y+">
             <BCType id = "all" label = "Symmetric" var = "symmetry"> </BCType>
           </Face>
           <Face side = "z-">
             <BCType id = "all" label = "Symmetric" var = "symmetry"> </BCType>
           </Face>
           <Face side = "z+">
             <BCType id = "all" label = "Symmetric" var = "symmetry"> </BCType>
           </Face>         
                  
       </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>[-0.01,0.0,-0.01]</lower>
              <upper>[0.04,0.03,0.04]</upper>
              <patches>[1,1,1]</patches>
              <extraCells>[0,0,0]</extraCells>
	      <resolution>[5,3,5]</resolution>
           </Box>
       </Level>
    </Grid>

</Uintah_specification>
