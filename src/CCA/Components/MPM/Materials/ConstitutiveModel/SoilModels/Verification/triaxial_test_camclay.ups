		<shear_modulus>1888888</shear_modulus>
		 <bulk_modulus>1416666</bulk_modulus>
      		</constitutive_model> -->

                <thermal_conductivity>1</thermal_conductivity>
                <specific_heat>1</specific_heat>

              <geom_object>

                  <box label="firstx-">
                     <min>[0.0,0.0,0.01]</min>
                     <max>[0.01,0.01,0.02]</max>
                  </box>

  		  <box label="secondx+">
                     <min>[0.02,0.0,0.01]</min>
                     <max>[0.03,0.01,0.02]</max>
                  </box>

		  <box label="thirdz-">
                     <min>[0.01,0.0,0.0]</min>
                     <max>[0.02,0.01,0.01]</max>
                  </box>

		  <box label="fourthz+">
                     <min>[0.01,0.0,0.02]</min>
                     <max>[0.02,0.01,0.03]</max>
                  </box>

                  <res>[1,1,1]</res>
                  <velocity>[0.0,0.0,0]</velocity>
                  <temperature>12</temperature>
               </geom_object>

           </material>

