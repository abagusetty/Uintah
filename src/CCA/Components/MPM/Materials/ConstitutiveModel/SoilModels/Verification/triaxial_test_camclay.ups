<?xml version="1.0" encoding="iso-8859-1"?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->

<!-- @version: Updated 7/31/00-->

<Uintah_specification>

   <Meta>
       <title>triaxial</title>
   </Meta>

   <SimulationComponent type="mpm" />

   <Time>
       <maxTime>5.00</maxTime>
       <initTime>0.0</initTime>
       <delt_min>0.00001</delt_min>
       <delt_max>0.00001</delt_max>
       <timestep_multiplier>0.02</timestep_multiplier>
   </Time>
   <DataArchiver>
       <filebase>triaxial.uda</filebase>
       <outputInterval>.01</outputInterval>
       <save label="p.x"/>
       <save label="p.velocity"/>
       <save label="p.volume"/>
       <save label="p.stress"/>
       <save label="p.particleID"/>
       <save label="g.mass"/>
       <save label="g.stressFS"/>
       <save label="RigidReactionForce"/>
       <save label="p.externalforce"/>   
   </DataArchiver>

    <MPM>
       <time_integrator>explicit</time_integrator>
       <use_load_curves>true</use_load_curves>
       <interpolator>linear</interpolator>
       <artificial_damping_coeff> 0.0 </artificial_damping_coeff>
    </MPM>

    <PhysicalConstants>
       <gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>
           <material>
              <density>7850.0</density>
                <constitutive_model type="rigid">
                </constitutive_model>
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>
              <geom_object>
                  <box label="piston">
                     <min>[0.0,0.01,0.0]</min>
                     <max>[0.03,0.02,0.03]</max>
                  </box>
                  <res>[1,1,1]</res>
                  <velocity>[0.0,0.0,0]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material>

           <material>
              <density>1600.0</density>
			  
	     
        <constitutive_model type="camclay">
          <equation_of_state type="borja_pressure">
            <p0> -9.0e4  </p0>
            <alpha> 60 </alpha>
            <kappatilde> 0.018 </kappatilde>
            <epse_v0> 0.0 </epse_v0>
          </equation_of_state>
          <shear_modulus_model type="borja_shear_modulus">
            <mu0> 5.4e6 </mu0>
            <p0> -9.0e4 </p0>
            <alpha> 60 </alpha>
            <kappatilde> 0.018 </kappatilde>
            <epse_v0> 0.0 </epse_v0>
          </shear_modulus_model>
          <yield_condition type="camclay_yield_function">
            <M> 1.05 </M>
          </yield_condition>
          <internal_variable_model type="borja_consolidation_pressure">
            <pc0> -9.0e4  </pc0>
            <lambdatilde> 0.13 </lambdatilde> 
            <kappatilde> 0.018 </kappatilde>
          </internal_variable_model>
        </constitutive_model>
		
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>

              <geom_object>
                  <box label="specimen_1">
                     <min>[0.01,0.0,0.01]</min>
                     <max>[0.02,0.01,0.02]</max>
                  </box>
                  <res>[1,1,1]</res>
                  <velocity>[0.0,0.0,0]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material>

	 <material>
    		<density>1675</density>
   	 	   
        <constitutive_model type="camclay">
          <equation_of_state type="borja_pressure">
            <p0> -9.0e4  </p0>
            <alpha> 60 </alpha>
            <kappatilde> 0.018 </kappatilde>
            <epse_v0> 0.0 </epse_v0>
          </equation_of_state>
          <shear_modulus_model type="borja_shear_modulus">
            <mu0> 5.4e6 </mu0>
            <p0> -9.0e4 </p0>
            <alpha> 60 </alpha>
            <kappatilde> 0.018 </kappatilde>
            <epse_v0> 0.0 </epse_v0>
          </shear_modulus_model>
          <yield_condition type="camclay_yield_function">
            <M> 1.05 </M>
          </yield_condition>
          <internal_variable_model type="borja_consolidation_pressure">
            <pc0> -9.0e4  </pc0>
            <lambdatilde> 0.13 </lambdatilde> 
            <kappatilde> 0.018 </kappatilde>
          </internal_variable_model>
        </constitutive_model>


