      		  <box label="2corner">
                     <min>[0.0,0.0,0.02]</min>
                     <max>[0.01,0.01,0.03]</max>
                  </box>

  		 <box label="3corner">
                     <min>[0.02,0.0,0.02]</min>
                     <max>[0.03,0.01,0.03]</max>
                  </box>

 		 <box label="4corner">
                     <min>[0.02,0.0,0.0]</min>
                     <max>[0.03,0.01,0.01]</max>
